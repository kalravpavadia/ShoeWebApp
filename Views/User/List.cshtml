@model IList<UserViewModel>
@{
}
<table>
    @foreach(var m in Model)
    {
        <tr>
            <td>
                @m.UserFirstName
            </td>
            <td>
                @m.UserLastName
            </td>
            <td>
                @m.UserId
            </td>
            <td>
                @m.UserEmail
            </td>
            <td>
                <a asp-controller="User" asp-action="Edit" asp-route-id="@m.UserId">Edit User</a>
            </td>
        </tr>
    }
</table>